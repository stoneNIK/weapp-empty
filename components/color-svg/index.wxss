.hik-svg-box {
  position: relative;
  display: flex;
  overflow: hidden;
}
.hik-svg-box .hik-svg {
  display: flex;
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: 50% 50%;
  transform: translateX(-2000px);
}
.hik-svg-box .hik-svg.help {
  background-image: url('data:image/svg+xml;base64,');
}
.hik-svg-box .hik-svg.warn {
  background-image: url('data:image/svg+xml;base64,');
}
.hik-svg-box .hik-svg.edit {
  background-image: url('data:image/svg+xml;base64,');
}
