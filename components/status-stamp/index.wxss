.stamp-wrap {
  transform-origin: 0 0;
  position: relative;
  width: 1000rpx;
  height: 890rpx;
  background-color: #fff;
  overflow: hidden;
}
.stamp-wrap .circle-out {
  position: absolute;
  bottom: 0;
  left: 20rpx;
  width: 980rpx;
  height: 980rpx;
  border: 40rpx solid #4C88FF;
  border-radius: 100%;
  box-sizing: border-box;
}
.stamp-wrap .circle-in {
  position: absolute;
  top: 80rpx;
  left: 190rpx;
  width: 640rpx;
  height: 640rpx;
  border: 20rpx solid #4C88FF;
  box-sizing: border-box;
  border-radius: 100%;
}
.stamp-wrap .text {
  position: absolute;
  top: 280rpx;
  left: 100rpx;
  width: 800rpx;
  height: 240rpx;
  text-align: center;
  line-height: 240rpx;
  font-size: 200rpx;
  color: #4C88FF;
  transform: rotate(-30deg);
  background-color: #fff;
  overflow: hidden;
  z-index: 3;
}
.stamp-wrap .star-wrap {
  position: absolute;
  top: 420rpx;
  left: 0;
  width: 180rpx;
  height: 270rpx;
  background-color: #fff;
  z-index: 2;
}
.stamp-wrap .star-wrap .star-five {
  margin-top: 70rpx;
  position: relative;
  display: block;
  color: #4C88FF;
  width: 0rpx;
  height: 0rpx;
  border-right: 50rpx solid transparent;
  border-bottom: 35rpx solid #4C88FF;
  border-left: 50rpx solid transparent;
  transform: rotate(35deg);
}
.stamp-wrap .star-wrap .star-five:before {
  border-bottom: 40rpx solid #4C88FF;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  position: absolute;
  height: 0;
  width: 0;
  top: -28rpx;
  left: -38rpx;
  display: block;
  content: '';
  transform: rotate(-35deg);
}
.stamp-wrap .star-wrap .star-five:after {
  position: absolute;
  display: block;
  color: #4C88FF;
  top: 3rpx;
  left: -55rpx;
  width: 0rpx;
  height: 0rpx;
  border-right: 50rpx solid transparent;
  border-bottom: 35rpx solid #4C88FF;
  border-left: 50rpx solid transparent;
  transform: rotate(-70deg);
  content: '';
}
.stamp-wrap .star-wrap .star-five + .star-five {
  margin: 80rpx 0 0 40rpx;
}
